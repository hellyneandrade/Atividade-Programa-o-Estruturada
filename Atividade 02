#include <stdio.h>

int main() {
    int n, i;
    long long int a = 1, b = 1, temp;

    printf("Digite o número do termo da sequência de Fibonacci: ");
    scanf("%d", &n);

    if (n <= 0) {
        printf("Por favor, digite um número inteiro positivo.\n");
    } else if (n == 1 || n == 2) {
        printf("O valor do termo %d é 1.\n", n);
    } else {
        for (i = 3; i <= n; i++) {
            temp = a + b;
            a = b;
            b = temp;
        }
        printf("O valor do termo %d é %lld.\n", n, b);
    }

    return 0;
}
